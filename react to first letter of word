#include <stdio.h>
#include <string.h>
int main()
{
    char ch[10];
    printf("enter the word\n");
    scanf("%s", ch);
    char fch = ch[0];
    printf("first letter of word is %c", fch);
    switch(fch)
    {
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        printf("vowel \n");
        break;
        default:
        printf("consonant \n");
        break;
    }
    return 0;
}
