#include <stdio.h>
int main()
{
    char alphabet;
    printf("enter the alphabet \n");
    scanf("%c",&alphabet);
    switch(alphabet)
    {
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        printf("vowel \n");
        break;
        default:
        printf("consonant \n");
        break;
    }
    return 0;
}
